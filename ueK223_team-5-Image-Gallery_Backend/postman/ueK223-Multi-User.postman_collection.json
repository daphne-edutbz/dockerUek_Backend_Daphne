      "name": "Auth",
      "item": [
        {
          "name": "Setup - Login as USER (daniel) → Token speichern",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "// Config data\r",
                  "let token = pm.response.headers.get(\"Authorization\");\r",
                  "\r",
                  "// Token in Environment Variable speichern\r",
                  "pm.environment.set(\"bearer_token_user\", token);\r",
                  "\r",
                  "console.log(\"Token gespeichert:\", token);\r",
                  "\r",
                  "// -- Tests --\r",
                  "\r",
                  "// -- Status test\r",
                  "pm.test(\"Status test\", function(){\r",
                  "    pm.response.to.have.status(200);\r",
                  "});\r",
                  "\r",
                  "// -- Header contains token\r",
                  "pm.test(\"Response contains token\", function(){\r",
                  "    pm.expect(token).to.be.string;\r",
                  "    pm.expect(token).to.not.be.empty;\r",
                  "});"
                ],
                "type": "text/javascript",
                "packages": {},
                "requests": {}
